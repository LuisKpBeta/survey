{
  "name": "survey",
  "version": "1.2.0",
  "description": "An survey application created using TDD",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "debug": "node --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "build": "rimraf dist && tsc",
    "up": "npm run build && docker-compose up --build -d",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuisCarlosb3/survey.git"
  },
  "author": "Luis Carlos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuisCarlosb3/survey/issues"
  },
  "homepage": "https://github.com/LuisCarlosb3/survey#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.5",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.20",
    "@types/node": "^13.13.9",
    "@types/supertest": "^2.0.9",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "eslint": "6",
    "eslint-config-standard-with-typescript": "^15.0.1",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "4",
    "git-commit-msg-linter": "^2.4.2",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.7",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.8",
    "validator": "^13.0.0"
  },
  "engines": {
    "node": "12.x"
  }
}
